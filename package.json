{
  "name": "@runspace/cli",
  "description": "Runspace daemon cli",
  "version": "0.1.2",
  "author": "Misha Marinenko @0x77dev",
  "bin": {
    "runspace": "./bin/run"
  },
  "bugs": "https://github.com/runspacegit/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-commands": "^1.2.3",
    "@oclif/plugin-help": "^3.0.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@runspace/daemon": "https://github.com/runspacegit/daemon.git#master",
    "@runspace/runtime": "https://github.com/runspacegit/runtime.git#master",
    "ataraxia": "^0.8.2",
    "ataraxia-local": "^0.8.2",
    "ataraxia-services": "^0.8.2",
    "ataraxia-tcp": "^0.8.2",
    "cli-ux": "^5.4.6",
    "tslib": "^1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^10",
    "@types/ws": "^7.2.4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "git-cz": "^4.6.2",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/runspacegit/cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "runspace",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "runspacegit/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "commit": "git-cz"
  },
  "types": "lib/index.d.ts"
}
